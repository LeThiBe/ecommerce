<tr>
  <td><%= order.id %></td>
  <td><%= order.created_at %></td>
  <td><%= order.address %></td>
  <td><%= order.email %></td>
  <td><%= order.phone %></td>
  <td><%= number_to_currency(order.total_price_all) %></td>
  <td class="text_<%= order.status %>">
    <%= order.status %></td>
  <td><%= link_to t(".view_order"), order %></td>
</tr>
